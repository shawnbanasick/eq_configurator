,[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Utils/UserNumberInput.jsx",["218"],"import React, { useState } from \"react\";\nimport styled from \"styled-components\";\nimport { view } from \"@risingstack/react-easy-state\";\nimport { useTranslation } from \"react-i18next\";\nimport appState from \"../GlobalState/appState\";\n\n// const clone = require(\"rfdc\")();\n\nconst UserNumberInput = (props) => {\n  const { t } = useTranslation();\n\n  const key = `${props.stateId}`; // ${props.sectionName}\n\n  // const [value, setValue] = useState(props.value);\n  const [showWarning, setShowWarning] = useState(false);\n\n  const handleChange = (e) => {\n    let value = e.target.value;\n    if (isNaN(value)) {\n      return null;\n    }\n\n    const key = props.stateId;\n    setShowWarning(false);\n    const upperLimit = props.upperLimit;\n    const lowerLimit = props.lowerLimit;\n    const upperLimitValue = props.upperLimit + props.step;\n    const lowerLimitValue = props.lowerLimit - props.step;\n\n    // limit according to props boundaries\n    if (value <= lowerLimitValue || value >= upperLimitValue) {\n      if (value < lowerLimit) {\n        value = lowerLimit;\n      }\n      if (value > upperLimit) {\n        value = upperLimit;\n      }\n      appState[key] = value; // setValue(value);\n      // appState[]\n      setShowWarning(true);\n    } else {\n      appState[key] = value; // setValue(value);\n    }\n\n    appState[key] = value;\n  };\n\n  const warningMessage = `${t(\"Lower Limit\")}: ${props.lowerLimit}, ${t(\n    \"Upper Limit\"\n  )}: ${props.upperLimit}`;\n\n  return (\n    <UserNumberContainer>\n      <TitleSpan>{`${t(props.label)} `}</TitleSpan>\n      <NumberInput\n        tabindex=\"0\"\n        placeholder={props.placeholder}\n        name={props.name}\n        step={props.step}\n        value={appState[key]}\n        onChange={handleChange}\n        className=\"optionsInput\"\n      />\n      {showWarning ? (\n        <NumberWarningMessage>{warningMessage}</NumberWarningMessage>\n      ) : null}\n    </UserNumberContainer>\n  );\n};\n\nexport default view(UserNumberInput);\n\nconst NumberInput = styled.input.attrs({\n  type: \"number\",\n})`\n  color: black;\n  cursor: pointer;\n  margin-bottom: 0;\n  width: 75px;\n  border-radius: 5px;\n  box-sizing: border-box;\n  height: 25px;\n  border: 1px solid lightgray;\n  box-shadow: none;\n  outline: none;\n  transition: 0.15s;\n  text-align: center;\n  &:hover {\n    outline: none;\n    background: none;\n    box-shadow: none;\n  }\n`;\n\nconst NumberWarningMessage = styled.div`\n  margin-left: 10px;\n  padding-top: 4px;\n  padding-left: 10px;\n  padding-right: 10px;\n  background-color: lightpink;\n  color: black;\n  height: 25px;\n  width: auto;\n  /* width: 225px; */\n`;\n\nconst UserNumberContainer = styled.div`\n  display: flex;\n  margin-top: 25px;\n  margin-left: 70px;\n  width: 900px;\n  flex-direction: row;\n  justify-content: flex-start;\n  align-items: center;\n`;\n\nconst TitleSpan = styled.span`\n  margin-right: 10px;\n`;\n","/media/smb/NewHDD1/CODE/htmlq_configurator/src/Utils/RadioButtons.jsx",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Utils/UserTextAreaInput.jsx",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Utils/UserTextInput.jsx",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Survey/Survey.jsx",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Firebase/FirebaseTextArea.jsx",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Map/convertQsortObjectToArray.js",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Survey/shouldDisplayObject.js",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Survey/FadeIn.js",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Survey/clearAddItemForm.js",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Map/InputDiv.jsx",[],"/media/smb/NewHDD1/CODE/htmlq_configurator/src/Sections/Survey/textQuestion.jsx",[],"/media/smb/NewHDD1/CODE/